 {{breadcrums 'Home' 'Product details'}}
<div class="app-content center-layout mt-2 mx-md-3 px-1">
<div class="content-wrapper">
	<div class="content-body">
	<section id="simple-user-cards" class="row pb-2">
		<div class="col-xl-12 col-md-6 col-sm-12">
		<div class="card-content123">
			<div class="row">	
			<span id="dots"></span>
			{{#each data as |format|}}
			<span class="col-xl-12 col-sm-12 col-12 app-content center-layout mt-2"> 
			<div class=" col-12  bg-white p-0">					 
		<div class="h3 p-1 font-weight-bolder"><i class="fa fa-user " aria-hidden="true"></i> {{#with ..}}{{__ "Basic Details" setlang}}{{/with}}
		</div>
		<div class="container-fluid "> 						  
				{{#if this.note}}
				<div class="row p-1"> 
				    <div class="col-sm-12">
				        <div class="row">
						  <div class="col-sm-3 font-weight-bold ">{{#with ..}}{{__ "Notes" setlang}}{{/with}}
						  </div>
						  <div class="col-sm-9"> 						  
						  {{#each this.note}}
						  <span class="colon">:</span>
							<div class="tabletext ">{{this}}</div><br>
						  {{/each}}
						  </div>
						</div>
				    </div>
				</div>
				{{/if}}
				{{#if this.fileattach}}
				<div class="row p-1"> 
				    <div class="col-sm-12">
				        <div class="row">
						  <div class="col-sm-3 font-weight-bold ">{{#with ..}}{{__ "Files atached" setlang}}{{/with}}
						  </div>
						  <div class="col-sm-9"> 						  
						  {{#each this.fileattach}}
						  <span class="colon">:</span>
							<div class="tabletext ">{{this}}</div><br>
						  {{/each}}
						  </div>
						</div>
				    </div>
				</div>
				{{/if}}
				{{if this.module}}
				<div class="row p-1"> 
				    <div class="col-sm-6">
				        <div class="row">
						  <div class="col-sm-6 font-weight-bold ">{{#with ..}}{{__ "Module" setlang}}{{/with}}
						  </div>
						  <div class="col-sm-6"> 
						  <span class="colon">:</span>
							<div class="tabletext ">{{this.module}}</div><br>
						  </div>
						</div>
				    </div>
				</div>
				{{/if}}
		</div>
	</div>
	{{/each}}
	</div>
	</div>
	</div>
	</section>
	</div>
	</div>
	</div>