				<input type="radio" name="reminder_will_send" class="custom-control-input" {{#ifCond this.reminder_will_send '==' 'After'}} checked="" value="{{this.reminder_will_send}}" {{/ifCond}} value="After"  id="no">
                <label class="custom-control-label" for="no">{{#with ..}}{{__ "After" setlang}}{{/with}}</label>
			</div> 
            </div>
			</div>
		</div>
		</div>
		</div>	 
				<div class="form-group col-md-9 mb-2 contact-repeater pl-0">
					<div data-repeater-list="reminderdata">
					{{#each this.reminderdata}}
						<div class="row" data-repeater-item>  
							<div class="col-md-4 ">
	                            <div class="col-12">    						
							        <div class="form-group input">
									<input class=" form-control t_d valid mb-2" value="1" type="number" min="1" max="5" name="number"  aria-invalid="false" {{#if this.number}} value="{{this.number}}" {{/if}}>
									</div>
								</div>	
						    </div> 
							<div class="col-md-4 ">
							   <div class="col-12 ">
